{
  "name": "Jason-Website",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.13.3"
  },
  "build": {
    "appId": "Jason-Website",
    "productName": "Your App Name",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/images/icon.icns"
    },
    "files": [
      "index.html",
      "main.js",
      "preload.js",
      "assets/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
